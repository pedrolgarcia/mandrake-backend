{
  "name": "mandrake-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "node ace build --production",
    "start": "node server.js",
    "dev": "node ace serve --watch",
    "migrate": "node ace migration:run",
    "seed": "node ace db:seed",
    "reset": "node ace migration:rollback && node ace migration:run && node ace db:seed",
    "migrate dev": "node ace migration:run && node ace db:seed && node ace serve --watch",
    "test": "node -r @adonisjs/assembler/build/register japaFile.ts",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.0.0",
    "@types/jsdom": "^16.2.10",
    "@types/supertest": "^2.0.11",
    "adonis-preset-ts": "^2.1.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-adonis": "^1.3.1",
    "eslint-plugin-prettier": "^3.4.0",
    "execa": "^5.0.0",
    "get-port": "^5.1.1",
    "japa": "^3.1.1",
    "jsdom": "^16.6.0",
    "pino-pretty": "^4.8.0",
    "prettier": "^2.3.0",
    "sqlite3": "^5.0.2",
    "supertest": "^6.1.3",
    "typescript": "~4.2",
    "youch": "^2.2.2",
    "youch-terminal": "^1.1.1"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.0.4",
    "@adonisjs/bouncer": "^2.2.1",
    "@adonisjs/core": "^5.1.0",
    "@adonisjs/lucid": "^14.1.0",
    "@adonisjs/repl": "^3.0.0",
    "luxon": "^1.27.0",
    "pg": "^8.6.0",
    "phc-argon2": "^1.1.1",
    "phc-bcrypt": "^1.0.7",
    "proxy-addr": "^2.0.6",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19"
  }
}
